        "idBuilder": "id_builder"
      },
      "processors": {
        "even": "processor1",
        "odd": "processor2"
      }
    },
    {
      "name": "processor1",
      "className": "xin.manong.stream.test.plugin.DummyHandler",
      "pluginConfig": {
        "name": "processor1"
      },
      "processors": {
      }
    },
    {
      "name": "processor2",
      "className": "xin.manong.stream.test.plugin.DummyHandler",
      "pluginConfig": {
        "name": "processor2"
      },
      "processors": {
      }
    }
  ]
}