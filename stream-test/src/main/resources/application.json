{
  "name": "fake_stream",
  "pythonEnv": "/Users/frankcl/Code/private/stream/stream-test/python",
  "loggerKeys": ["id"],
  "resources": [
    {
      "name": "id_builder",
      "className": "xin.manong.stream.test.resource.AutoIncreasedIDResource"
    }
  ],
  "receivers": [
    {
      "name": "receiver",
      "receiverClass": "xin.manong.stream.boost.receiver.fake.FakeReceiver",
      "receiverConfigMap": {
        "timeIntervalMs": 1000,
        "threadNum": 2
      },
      "processors": ["record_id_builder"]
    }
  ],
  "processors": [
    {
      "name": "record_id_builder",
      "className": "xin.manong.stream.test.plugin.RecordIDBuilder",
      "pluginConfig": {
        "idBuilder": "id_builder"
      },
      "processors": {
        "even": "python_processor",
        "odd": "processor2"
      }
    },
    {
      "name": "processor1",
      "className": "xin.manong.stream.test.plugin.DummyHandler",
      "pluginConfig": {
        "name": "processor1"
      },
      "processors": {
      }
    },
    {
      "name": "processor2",
      "className": "xin.manong.stream.test.plugin.DummyHandler",
      "pluginConfig": {
        "name": "processor2"
      },
      "processors": {
      }
    },
    {
      "name": "python_processor",
      "className": "xin.manong.stream.sdk.python.PythonPlugin",
      "pluginConfig": {
        "__PYTHON_FILE__": "test_plugin.py",
        "__PYTHON_CLASS__": "TestPlugin"
      },
      "processors": {
        "next": "processor1"
      }
    }
  ]
}